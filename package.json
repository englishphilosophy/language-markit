{
  "name": "language-markit",
  "version": "1.0.0",
  "description": "Markit language support for Atom and VSCode",
  "publisher": "englishphilosophy",
  "keywords": [
    "language",
    "grammar",
    "markit",
    "tei"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/englishphilosophy/language-markit"
  },
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0",
    "vscode": "^1.43.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "markit",
      "aliases": ["Markit", "markit"],
      "extensions": [".mit"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "markit",
      "scopeName": "source.markit",
      "path": "./grammars/markit.json",
      "embeddedLanguages": {
        "meta.embedded.block.yaml": "yaml"
      }
    }]
  }
}
